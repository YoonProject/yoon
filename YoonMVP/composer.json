{
    "name": "yoon/yoonmvp",
    "type": "standard",
    "description": "The yoon mvp project.",
    "homepage": "https://yoon.online",
    "license": "MIT",
    "require": {
        "psr/log": "^1.0.2",
        "byjg/micro-orm": "2.0.*",
        "pleonasm/merkle-tree": "^1.0",
        "php-di/php-di": "^6.0",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/promises": "^1.3",
        "maciejczyzewski/bottomline": "^0.1.1",
        "ramsey/uuid": "^3.7",
        "silex/silex": "~1.2"
    },
    "require-dev": {
        "doctrine/common": "^2.8.1",
        "phake/phake": "2.*",
        "swiftmailer/swiftmailer": "4.*",
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Yoon\\YoonMvp\\": "src/",
            "__\\": "vendor/maciejczyzewski/bottomline/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPUnit\\": "vendor/phpunit/phpunit/src/",
            "Yoon\\YoonMvp\\": "src/",
            "Yoon\\YoonMvp\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/yoon"
    ],
    "authors": [
        {
            "name": "Philip Meeske",
            "email": "philip.meeske@yoon.online",
            "homepage": "https://yoon.online"
        }
    ]
}
