{
    "name": "yoon/yoonmvp",
    "type": "standard",
    "description": "The yoon mvp project.",
    "homepage": "https://yoon.online",
    
    "license": "MIT",
    "require": {
        "psr/log": "^1.0.2",
        "byjg/micro-orm": "2.0.*",
        "pleonasm/merkle-tree": "^1.0",
        "php-di/php-di": "^6.0",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/promises": "^1.3",
        "maciejczyzewski/bottomline": "^0.1.1",
        "ramsey/uuid": "^3.7",
        "silex/silex": "~1.2",
        "hoa/stream": "1.17.02.21",
        "doctrine/annotations": "^1.6",
        "pitsolu/merkle-tree": "dev-master"
    },
    "require-dev": {
        "doctrine/common": "^2.8.1",
        "phake/phake": "2.*",
        "swiftmailer/swiftmailer": "4.*",
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Yoon\\YoonPublic\\": "public/",
            "Yoon\\YoonMvp\\": "src/",
            "ZuluCrypto\\StellarSdk\\" : "stellar-api/src",
            "Swagger\\Client\\" : "chaintpoint-api/SwaggerClient-php/lib/",
            "__\\": "vendor/maciejczyzewski/bottomline/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPUnit\\": "vendor/phpunit/phpunit/src/",
            "Yoon\\YoonPublic\\": "public/",
            "Yoon\\YoonMvp\\": "src/",
            "ZuluCrypto\\StellarSdk\\" : "stellar-api/src",
            "Swagger\\Client\\" : "chaintpoint-api/SwaggerClient-php/lib/",
            "Yoon\\YoonMvp\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/yoon"
    ],
    "authors": [
        {
            "name": "Philip Meeske",
            "email": "philip.meeske@yoon.online",
            "homepage": "https://yoon.online"
        }
    ]
}
